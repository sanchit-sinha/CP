// finding prime factors in sqrt logn
vector<ll> prime;
ll temp=x;
for(ll i=2;i<=sqrt(x);i++){
    if(temp%i==0){
        while(temp%i==0) temp/=i;
        prime.pb(i);

    }
}
if(temp!=1) prime.pb(temp);
]]></content>
